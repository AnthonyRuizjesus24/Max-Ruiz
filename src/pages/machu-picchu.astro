---
  import Header from '../components/Header.astro';
  import Footer from '../components/Footer.astro';
  import '../styles/global.css';
  import '../components/Style-Machu-Picchu.css';
  import BaseLayout from '../layouts/BaseLayout.astro';
  import Greeting from '../components/Greeting';
  import Button from '../components/Button.astro';
  import MachuPicchuInfo from '../components/react/MachuPicchuInfo';
---

<BaseLayout pageTitle='Informacion de Machu Picchu'>
    <Greeting />
    <html lang="es">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Collage</title>
    <link rel="stylesheet" href="../src/styles/global.css" />
  </head>
  <body>
  
    <div class="collage">
      <div> <img src="/imagen/inicio.jpg" alt="Peru" class="custom-image" /></div>
      <img src="../imagen/machu-picchu/imagen01.jpg" alt="imagen01" class="custom-image"/>
      <img src="../imagen/machu-picchu/imagen02.jpg" alt="imagen01" class="custom-image"/>
      <img src="../imagen/machu-picchu/imagen03.jpg" alt="imagen01" class="custom-image"/>
      <img src="../imagen/machu-picchu/imagen04.jpg" alt="imagen01" class="custom-image"/>
    </div>
  </body>
</html>
    <div class="video-container">
      <iframe width="560" height="315" src="https://www.youtube.com/embed/_NqUHT4qE8w" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    </div>
    <Button />
    <MachuPicchuInfo />
  </BaseLayout>